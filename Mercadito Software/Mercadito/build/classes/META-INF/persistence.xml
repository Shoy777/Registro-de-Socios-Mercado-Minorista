<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="Mercadito">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>bean.Alquiler</class>
		<class>bean.DetalleAlquiler</class>
		<class>bean.DetalleAlquilerPK</class>
		<class>bean.DetallePagare</class>
		<class>bean.DetallePagarePK</class>
		<class>bean.Dominio</class>
		<class>bean.Establecimiento</class>
		<class>bean.Pagare</class>
		<class>bean.Socio</class>
		<class>bean.Ubigeo</class>
		<class>bean.Usuario</class>
		
		<properties>
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/mercadito" />
			<property name="eclipselink.jdbc.user" value="root" />
			<property name="eclipselink.jdbc.password" value="mysql" />
   			<property name="eclipselink.logging.level" value="FINE"/>
		</properties>
	</persistence-unit>
</persistence>
